public class  {

    private RemoteWebDriver driver;

    public (RemoteWebDriver driver){
        this.driver = driver;
    }

        private By tablet = By.name("name=btnK");  
      
        private By magnet = By.name("name=btnI");  
      
        private By kingkong = By.name("name=q");  
      

        public RemoteWebElement gettablet(){
            return driver.findElement(tablet);
        }      
        public RemoteWebElement getmagnet(){
            return driver.findElement(magnet);
        }      
        public RemoteWebElement getkingkong(){
            return driver.findElement(kingkong);
        }      
}


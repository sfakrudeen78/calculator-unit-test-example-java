,        {"value":[{"class":"Button","frameName":"","locator":{"tag_name":"INPUT","type":"name","value":"name=btnI"},"name":"lat","strict_locator":"name=btnI"}]}
        

        ]
    }

    PO*/

    private By abc = By.name("name=btnK");  
      
,    private By lat = By.name("name=btnI");  
      

    public RemoteWebElement getabc(){
        return driver.findElement(abc);
    }      
,    public RemoteWebElement getlat(){
        return driver.findElement(lat);
    }      
}


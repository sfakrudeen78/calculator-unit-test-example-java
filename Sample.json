    private By tab = By.css("css=div.FPdoLc.VlcLAe > center");  
      
    private By mob = By.name("name=q");  
      

    public RemoteWebElement gettab(){
        return driver.findElement(tab);
    }      
    public RemoteWebElement getmob(){
        return driver.findElement(mob);
    }      

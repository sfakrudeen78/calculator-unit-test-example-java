    private By uma = By.name("name=btnK");  
      
,    private By tab = By.name("name=btnI");  
      

    public RemoteWebElement getuma(){
        return driver.findElement(uma);
    }      
,    public RemoteWebElement gettab(){
        return driver.findElement(tab);
    }      

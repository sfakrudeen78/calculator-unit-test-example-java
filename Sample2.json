public class Sample2 {

    private RemoteWebDriver driver;

    public Sample2(RemoteWebDriver driver){
        this.driver = driver;
    }

        private By ab = By.name("name&#x3D;btnK");  
      
        private By bf = By.name("name&#x3D;btnI");  
      
        private By lab = By.name("name&#x3D;q");  
      

        public RemoteWebElement getab(){
            return driver.findElement(ab);
        }      
        public RemoteWebElement getbf(){
            return driver.findElement(bf);
        }      
        public RemoteWebElement getlab(){
            return driver.findElement(lab);
        }      
}

